    <bpmn:sequenceFlow id="Flow_No" name="No" sourceRef="Gateway_Approval" targetRef="EndEvent_Rejected" />
    <bpmn:sequenceFlow id="Flow_Timeout" sourceRef="BoundaryEvent_48h" targetRef="EndEvent_Rejected" />
    <bpmn:endEvent id="EndEvent_Rejected" name="Booking Cancelled">
      <bpmn:incoming>Flow_No</bpmn:incoming>
      <bpmn:incoming>Flow_Timeout</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Success" name="Booking Confirmed">
      <bpmn:incoming>Flow_Yes</bpmn:incoming>
    </bpmn:endEvent>
