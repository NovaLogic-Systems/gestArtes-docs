  <bpmn:process id="Process_5" isExecutable="false">
    <bpmn:laneSet id="LaneSet_5">
      <bpmn:lane id="Lane_Buyer" name="Interested">
        <bpmn:flowNodeRef>Start_Marketplace</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InitiateContact</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PerformPayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Success</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Seller" name="Item Owner">
        <bpmn:flowNodeRef>Task_NegotiateTerms</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="Start_Marketplace" name="Interest in Marketplace Item">
      <bpmn:outgoing>Flow_M1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_InitiateContact" name="Send Interest Request">
      <bpmn:incoming>Flow_M1</bpmn:incoming>
      <bpmn:outgoing>Flow_M2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_NegotiateTerms" name="Confirm Terms of Exchange">
      <bpmn:incoming>Flow_M2</bpmn:incoming>
      <bpmn:outgoing>Flow_M3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_PerformPayment" name="Pay via External Gateway (or Cash)">
      <bpmn:incoming>Flow_M3</bpmn:incoming>
      <bpmn:outgoing>Flow_M4</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_M1" sourceRef="Start_Marketplace" targetRef="Task_InitiateContact" />
    <bpmn:sequenceFlow id="Flow_M2" sourceRef="Task_InitiateContact" targetRef="Task_NegotiateTerms" />
    <bpmn:sequenceFlow id="Flow_M3" sourceRef="Task_NegotiateTerms" targetRef="Task_PerformPayment" />
    <bpmn:sequenceFlow id="Flow_M4" sourceRef="Task_PerformPayment" targetRef="End_Success" />
    <bpmn:endEvent id="End_Success" name="Transaction Completed">
      <bpmn:incoming>Flow_M4</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
